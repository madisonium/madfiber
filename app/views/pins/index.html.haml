- content_for :stylesheets do
  = stylesheet_link_tag 'pins'
- content_for :javascripts do
  :javascript
    var authenticity_token = '#{form_authenticity_token}';
  = javascript_include_tag 'pins'

%table
  %thead
    %tr
      %th{:colspan => 4} Unapproved
    %tr
      %th Name
      %th Address
      %th Zip
      %th Actions
  %tbody
    = render :partial => 'unapproved', :collection => @unapproved_pins, :as => :pin
%hr
%table
  %thead
    %tr
      %th{:colspan => 5} Approved
    %tr
      %th Name
      %th Address
      %th Zip
      %th Lat/Lng
      %th Actions
  %tbody
    = render :partial => 'approved', :collection => @approved_pins, :as => :pin
